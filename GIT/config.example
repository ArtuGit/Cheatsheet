[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
  autocrlf = true
  safecrlf = false	
[user]
	name = <<FirstName LastName>>
	email = <<Your email>>
[remote "origin"]
	url = https://<<URL>>
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
